@model IEnumerable<LabReservation.UI.Areas.Admin.Models.BranchModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <thead>
        <tr>
           
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LabName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AreaName)
            </th>
           
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
          
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LabName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AreaName)
            </td>           
            <td>
                @Html.ActionLink("Edit", "Edit", new { BranchId = item.BranchId }) |
                @Html.ActionLink("Delete", "Delete", new { BranchId = item.BranchId })
            </td>
        </tr>
}
    </tbody>
</table>
